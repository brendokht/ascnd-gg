{
  "name": "@ascnd-gg/database",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "scripts": {
    "ts:build": "tsc --build tsconfig.build.json",
    "ts:build-watch": "tsc --build tsconfig.build.json --watch",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --skip-generate",
    "db:reset": "prisma migrate reset --skip-seed",
    "db:seed": "prisma db seed",
    "lint": "eslint . --max-warnings 0",
    "format": "prettier --write .",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@ascnd-gg/eslint-config": "workspace:*",
    "@ascnd-gg/typescript-config": "workspace:*",
    "@types/node": "^24.7.0",
    "eslint": "^9.37.0",
    "prettier": "^3.6.2",
    "prisma": "^6.16.3",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "ts-node": "^10.9.2"
  }
}
