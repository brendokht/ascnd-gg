{
  "name": "ascnd-gg",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev:compose dev",
    "dev:compose": "docker compose --env-file .env.development.local up",
    "compose": "docker compose --env-file .env.development.local up",
    "down": "docker compose --env-file .env.development.local down",
    "stop": "docker compose --env-file .env.development.local stop",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "check-types": "turbo run check-types",
    "prepare": "husky",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@ascnd-gg/eslint-config": "workspace:*",
    "@ascnd-gg/typescript-config": "workspace:*",
    "eslint": "^9.33.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "turbo": "^2.0.12",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
